{
  "name": "template_spa",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "api": "mh down && mh up sample_api",
    "service": "mh down && mh up sample && npm run open",
    "container": "DOCKER_BUILDKIT=0 docker build --build-arg GITHUB_TOKEN=$GITHUB_TOKEN -t ghcr.io/agile-learning-institute/mentorhub_sample_spa:latest .",
    "push": "docker push ghcr.io/agile-learning-institute/mentorhub_sample_spa:latest",
    "open": "open -a 'Google Chrome' 'http://localhost:8390' || google-chrome 'http://localhost:8390' || exg-open 'localhost:8390'",
    "build-publish": "npm install --include=dev && npm run container && npm run push"
  },
  "dependencies": {
    "@agile-learning-institute/mentorhub_spa_utils": "^0.1.0",
    "@tanstack/vue-query": "^5.59.0",
    "pinia": "^2.2.6",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vuetify": "^3.11.3",
    "@mdi/font": "^7.4.47"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/test-utils": "^2.4.6",
    "@vitest/ui": "^3.0.5",
    "@vitest/coverage-v8": "^3.0.5",
    "typescript": "^5.9.2",
    "vite": "^7.2.0",
    "vitest": "^3.0.5",
    "vue-tsc": "^2.2.0",
    "cypress": "^15.8.0",
    "jsdom": "^25.0.1"
  },
  "overrides": {
    "qs": "^6.14.1"
  }
}